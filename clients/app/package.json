{
  "name": "viva-app",
  "version": "1.4.0",
  "private": true,
  "scripts": {
    "postinstall": "npm install --prefix src-cordova",
    "serve": "vue-cli-service serve --mode development",
    "build": "vue-cli-service build --mode development",
    "production": "vue-cli-service build --mode production",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "cordova-add-ios": "cordova platform add ios",
    "cordova-build-ios": "cross-env CORDOVA_PLATFORM=ios vue-cli-service cordova-build-ios --mode production",
    "cordova-build-ios-development": "cross-env CORDOVA_PLATFORM=ios vue-cli-service cordova-build-ios --mode development",
    "cordova-build-android": "cross-env CORDOVA_PLATFORM=android vue-cli-service cordova-build-android --mode production --buildConfig",
    "cordova-build-android-development": "cross-env CORDOVA_PLATFORM=android vue-cli-service cordova-build-android --mode development",
    "cordova-build-browser": "cross-env CORDOVA_PLATFORM=browser vue-cli-service cordova-build-browser",
    "cordova-build-only-www-android": "cross-env CORDOVA_PLATFORM=android vue-cli-service cordova-build-only-www-android",
    "cordova-build-only-www-browser": "cross-env CORDOVA_PLATFORM=browser vue-cli-service cordova-build-only-www-browser",
    "cordova-build-only-www-ios": "cross-env CORDOVA_PLATFORM=ios vue-cli-service cordova-build-only-www-ios",
    "cordova-build-only-www-osx": "cross-env CORDOVA_PLATFORM=osx vue-cli-service cordova-build-only-www-osx",
    "cordova-build-osx": "cross-env CORDOVA_PLATFORM=osx vue-cli-service cordova-build-osx",
    "cordova-prepare": "vue-cli-service cordova-prepare",
    "cordova-serve-android": "cross-env CORDOVA_PLATFORM=android vue-cli-service cordova-serve-android",
    "cordova-serve-browser": "cross-env CORDOVA_PLATFORM=browser vue-cli-service cordova-serve-browser",
    "cordova-serve-ios": "cross-env CORDOVA_PLATFORM=ios vue-cli-service cordova-serve-ios",
    "cordova-serve-osx": "cross-env CORDOVA_PLATFORM=osx vue-cli-service cordova-serve-osx",
    "server": "vue-cli-service serve --port 8090"
  },
  "dependencies": {
    "@kazupon/vue-i18n-loader": "^0.4.1",
    "@vue/cli": "^4.0.5",
    "axios": "^0.19.2",
    "clipboard-polyfill": "^2.8.6",
    "cordova": "^9.0.0",
    "core-js": "^3.3.2",
    "dotenv": "^7.0.0",
    "eslint-plugin-jest": "^23.0.2",
    "idb": "^4.0.4",
    "indexeddb-getall-shim": "^1.3.5",
    "inobounce": "^0.2.0",
    "jimp": "^0.5.6",
    "lodash": "^4.17.15",
    "moment": "^2.29.1",
    "qrcode-reader": "^1.0.4",
    "tus-js-client": "^1.8.0",
    "vue": "^2.6.10",
    "vue-draggable": "^2.0.2",
    "vue-i18n": "^8.15.0",
    "vue-router": "^3.1.3",
    "vuex": "^3.0.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.0",
    "@vue/cli-plugin-vuex": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^6.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "tailwindcss": "^1.3.5",
    "vue-cli-plugin-cordova": "^2.3.6",
    "vue-template-compiler": "^2.6.10"
  }
}
